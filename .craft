# Craft configuration file (https://github.com/kilianpaquier/craft)
# yaml-language-server: $schema=https://raw.githubusercontent.com/kilianpaquier/craft/beta/.schemas/craft.schema.json
bot: renovate
ci:
  auth:
    maintenance: github-token
    release: github-token
  name: github
  options:
    - codecov
    - codeql
    - labeler
  release: {}
exclude:
  - chart
  - shell
include:
  - renovate:post-upgrade
license: mit
maintainers:
  - name: kilianpaquier
platform: github
